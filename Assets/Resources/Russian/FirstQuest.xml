    <answer text="Есть Работенка?" toNode="3" questValue="0" questName="FirstQuest" />
    <answer text="Есть еще задания?" toNode="4" questValue="665" questName="FirstQuest" />
    <answer text="Тут это..." toNode="5" questValue="4" questName="WolfQuest" />
    <answer text="На счет задания" toNode="6" questValue="3" questName="WolfQuest" />
    <answer text="Птицы перестреляны" toNode="10" questStatus="3" questValue="2" questName="FirstQuest" />
    <answer text="Есть еще задания?" toNode="4" questValue="777" questName="FirstQuest" />
    <answer text="Пока" exit="True" />
  </node>
  <node id="1" npcText="Неплохо">
    <answer text="Понятно" toNode="0" />
    <answer text="Иди в жопу" exit="True" />
  </node>
  <node id="2" npcText="Да, что с заданием?">
